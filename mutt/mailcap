text/html;                      links %s; nametemplate=%s.html
text/html;                      links -dump %s; nametemplate=%s.html; copiousoutput


application/pdf; { set -m \; /bin/mv -T %s %s.mv \; ( evince %s.mv >/dev/null 2>&1 \; /bin/rm %s.mv \; ) & } \; disown -a

image/jpeg; feh %s
image/gif; feh %s 
image/GIF; feh %s 
image/JPG; feh %s 
image/jpg; feh %s 
image/png; feh %s 
